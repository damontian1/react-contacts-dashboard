eg); }\n  100% { transform: rotate(360deg); }\n}\n$spaceamounts: (0, 1, 2, 3, 4, 5);\n$ranges: (\n    0: \"0\",\n    1: \"0.25\",\n    2: \"0.5\",\n    3: \"1\",\n    4: \"1.5\",\n    5: \"3\"\n);\n$sides: (\"top\", \"bottom\", \"left\", \"right\", \"x\", \"y\", \"\");\n\n@each $space in $spaceamounts {\n  @each $side in $sides {\n    .m#{str-slice($side, 0, 1)}-#{$space} {\n      @if ($side == \"y\") {\n\t\t \tmargin-top: #{map-get($ranges, $space)}rem !important;\n      \tmargin-bottom: #{map-get($ranges, $space)}rem !important;\n      } @else if ($side == \"x\") {\n      \tmargin-left: #{map-get($ranges, $space)}rem !important;\n      \tmargin-right: #{map-get($ranges, $space)}rem !important;\n      }\n      @else if ($side == \"\") {\n      \tmargin: #{map-get($ranges, $space)}rem !important;\n      } @else {\n      \tmargin-#{$side}: #{map-get($ranges, $space)}rem !important;\n      }\n    }\n    .p#{str-slice($side, 0, 1)}-#{$space} {\n      @if ($side == \"y\") {\n      \tpadding-top: #{map-get($ranges, $space)}rem !important;\n      \tpadding-bottom: #{map-get($ranges, $space)}rem !important;\n      } @else if ($side == \"x\") {\n\t\t \tpadding-left: #{map-get($ranges, $space)}rem !important;\n      \tpadding-right: #{map-get($ranges, $space)}rem !important;\n      }\n      @else if ($side == \"\") {\n      \tpadding: #{map-get($ranges, $space)}rem !important;\n      } @else {\n      \tpadding-#{$side}: #{map-get($ranges, $space)}rem !important;\n      }\n    }\n  }\n}\n/*-----------------------\n\tDESKTOP\n-----------------------*/\nsection#navigation {\n  box-shadow: 0 4px 20px rgba(50,50,93,.50);\n  nav {\n    text-align: center;\n    & > div.row {\n      display: flex;\n      align-items: center;\n    }\n  }\n  ul {\n    list-style: none;\n  }\n  #logo {\n    width: 52px;\n    padding: 0.2em 0em;\n  }\n}\nsection#api-info-sidebar {\n}\nsection#contactForm {\n  form {\n    width: 100%;\n    margin: 0 auto;\n  }\n  .input-group {\n    margin: 1em 0em;\n    width: 100%\n  }\n  .input-group-addon {\n    width: 150px\n  }\n  .contact-form-field {\n    width: 100%\n  }\n  .contact-form-button {\n    position: relative;\n    top: 0;\n    left: 50%;\n    width: 80%;\n    transform: translateX(-50%);\n    margin-top: 2em;\n  }\n}\n.weather-icons {\n  width: 100%;\n}\n.news-widget__cards {\n  display: grid;\n  grid-template-columns: 1fr;\n  grid-row-gap: 1rem;\n  justify-items: center;\n  .news-image {\n    width: 100%;\n    max-width: 350px;\n  }\n}\n/*-----------------------\n\tMOBILE\n-----------------------*/\n@media (max-width: 992px){\n  .weather-icons {\n    width: 100px;\n  }\n  .news-image {\n    width: 150px;\n  }\n  section#navigation {\n    nav {\n      text-align: center;\n      padding: 1em 0em;\n    }\n  }\n  .news-widget__cards {\n    .news-image {\n      max-width: 450px;\n    }\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"9bf1b08edd27e00997a5cfbc82df9c89","cacheData":{"env":{}}}